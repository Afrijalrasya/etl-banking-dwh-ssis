      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{93708732-5AF7-4A3E-992A-29DBE41929CA}"
          SQLTask:SqlStatementSource="IF OBJECT_ID('gold.dim_branch', 'V') IS NOT NULL&#xA;DROP VIEW gold.dim_branch;&#xA;GO&#xA;CREATE VIEW gold.dim_branch AS&#xA;SELECT&#xA; ROW_NUMBER() OVER (order by branch_code) as id_branch,&#xA; branch_code as kode_branch&#xA;FROM(&#xA;SELECT DISTINCT&#xA; branch_code&#xA;FROM silver.transaksi&#xA;)t" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
