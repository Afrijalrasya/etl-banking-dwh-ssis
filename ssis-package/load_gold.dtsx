          SQLTask:SqlStatementSource="IF OBJECT_ID('gold.dim_channel', 'V') IS NOT NULL&#xA;DROP VIEW gold.dim_channel;&#xA;GO&#xA;CREATE VIEW gold.dim_channel AS&#xA;SELECT&#xA; ROW_NUMBER() OVER(ORDER BY nama_channel) as id_channel, --surrogate key&#xA; nama_channel&#xA;FROM(&#xA;SELECT DISTINCT&#xA; channel as nama_channel&#xA;FROM silver.transaksi&#xA;)t" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
