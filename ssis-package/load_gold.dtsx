      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{93708732-5AF7-4A3E-992A-29DBE41929CA}"
          SQLTask:SqlStatementSource="IF OBJECT_ID('gold.fact_transaksi', 'V') IS NOT NULL&#xA;DROP VIEW gold.fact_transaksi;&#xA;GO&#xA;CREATE VIEW gold.fact_transaksi AS&#xA;select&#xA;t.id_transaksi,&#xA;t.nomor_akun,&#xA;t.tanggal_transaksi,&#xA;CAST(t.jumlah AS bigint)as jumlah,&#xA;c.id_channel,&#xA;b.id_branch&#xA;from silver.transaksi t&#xA;LEFT JOIN gold.dim_branch b&#xA;on b.kode_branch = t.branch_code&#xA;LEFT JOIN gold.dim_channel c&#xA;on c.nama_channel = t.channel" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\(G) quick report_transaksi"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{02bbac22-4f08-401b-8054-8f5ceea5edb9}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="(G) quick report_transaksi"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{93708732-5AF7-4A3E-992A-29DBE41929CA}"
          SQLTask:SqlStatementSource="--create laporan_transaksi=====================================================================&#xA;IF OBJECT_ID('gold.laporan_transaksi', 'V') IS NOT NULL&#xA;DROP VIEW gold.laporan_transaksi;&#xA;GO&#xA;&#xA;CREATE VIEW gold.laporan_transaksi AS&#xA;&#xA;WITH base_query AS(&#xA;select&#xA;t.id_transaksi,&#xA;t.nomor_akun,&#xA;t.tanggal_transaksi,&#xA;CAST(t.jumlah AS bigint)as jumlah,&#xA;c.id_channel,&#xA;c.nama_channel,&#xA;b.id_branch,&#xA;b.kode_branch&#xA;from gold.fact_transaksi t&#xA;LEFT JOIN gold.dim_branch b&#xA;on b.id_branch = t.id_branch&#xA;LEFT JOIN gold.dim_channel c&#xA;on c.id_channel = t.id_channel&#xA;),&#xA;aggregate_data as(&#xA;SELECT&#xA;*,&#xA;DAY(tanggal_transaksi) as date_day,&#xA;MONTH(tanggal_transaksi) as date_month,&#xA;SUM(jumlah)over(PARTITION BY tanggal_transaksi) as total_cost_transaksi_harian,&#xA;AVG(jumlah)over(PARTITION BY tanggal_transaksi) as avg_transaksi_harian,&#xA;COUNT(id_transaksi) OVER (PARTITION by tanggal_transaksi) as freq_transaksi_harian,&#xA;COUNT(id_transaksi) OVER (partition by nama_channel)as freq_transaksi_by_channel,&#xA;COUNT(id_transaksi) OVER (PARTITION by MONTH(tanggal_transaksi)) as freq_transaksi_bulanan&#xA;FROM base_query&#xA;)&#xA;SELECT&#xA;id_transaksi,&#xA;nomor_akun,&#xA;tanggal_transaksi,&#xA;jumlah,&#xA;id_channel,&#xA;nama_channel,&#xA;id_branch,&#xA;kode_branch,&#xA;total_cost_transaksi_harian,&#xA;avg_transaksi_harian,&#xA;freq_transaksi_harian,&#xA;freq_transaksi_by_channel,&#xA;freq_transaksi_bulanan,&#xA;AVG(freq_transaksi_harian) over(partition by date_day) as avg_freq_transaksi_harian&#xA;FROM aggregate_data" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
